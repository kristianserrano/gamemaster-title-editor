{
  "id": "gamemaster-title-editor",
  "title": "Gamemaster Title Editor",
  "version": "1.0.0",
  "description": "Allows you to customize the Gamemaster's title (e.g., \"The Keeper\" instead of \"GM\"). This replaces the title in the Players List and A/V panel.",
  "url": "https://www.patreon.com/KristianSerrano",
  "download": "",
  "manifest": "",
  "compatibility": {
    "minimum": "11",
    "verified": "12"
  },
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json",
      "flags": {}
    }
  ],
  "esmodules": [
    "modules/gamemaster-title-editor.js"
  ],
  "flags": {
    "hotReload": {
      "extensions": [
        "json",
        "js"
      ],
      "paths": [
        "lang/en.json",
        "modules/gamemaster-title-editor.js"
      ]
    }
  }
}
